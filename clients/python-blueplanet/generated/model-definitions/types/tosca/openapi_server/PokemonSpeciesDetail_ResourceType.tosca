          type = array
          items.type = AbilityDetailPokemonInnerPokemon
          description = ""
          optional = true
        }
        color {
          type = PokemonColorSummary
          description = ""
          optional = true
        }
        shape {
          type = PokemonShapeSummary
          description = ""
          optional = true
        }
        evolves_from_species {
          type = PokemonSpeciesSummary
          description = ""
          optional = true
        }
        evolution_chain {
          type = EvolutionChainSummary
          description = ""
          optional = true
        }
        habitat {
          type = PokemonHabitatSummary
          description = ""
          optional = true
        }
        generation {
          type = GenerationSummary
          description = ""
          optional = true
        }
        names {
          type = array
          items.type = PokemonFormDetailFormNamesInner
          description = ""
          optional = true
        }
        pal_park_encounters {
          type = array
          items.type = PokemonSpeciesDetailPalParkEncountersInner
          description = ""
          optional = true
        }
        form_descriptions {
          type = array
          items.type = PokemonSpeciesDescription
          description = ""
          optional = true
        }
        flavor_text_entries {
          type = array
          items.type = PokemonSpeciesFlavorText
          description = ""
          optional = true
        }
        genera {
          type = array
          items.type = PokemonSpeciesDetailGeneraInner
          description = ""
          optional = true
        }
        varieties {
          type = array
          items.type = PokemonSpeciesDetailVarietiesInner
          description = ""
          optional = true
        }
    }
  }
}

serviceTemplates {

  PokemonSpeciesDetail {
    title = PokemonSpeciesDetail
    description = PokemonSpeciesDetail
    implements = openapi_server.resourceTypes.PokemonSpeciesDetail

  }
}
