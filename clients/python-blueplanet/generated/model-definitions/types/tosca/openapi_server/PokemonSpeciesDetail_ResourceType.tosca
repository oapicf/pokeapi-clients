    properties {
        id {
          type =  integer
          description = ""
          optional = true
        }
        name {
          type =  string
          description = ""
          optional = true
        }
        order {
          type =  integer
          description = ""
          optional = false
        }
        gender_rate {
          type =  integer
          description = ""
          optional = false
        }
        capture_rate {
          type =  integer
          description = ""
          optional = false
        }
        base_happiness {
          type =  integer
          description = ""
          optional = false
        }
        is_baby {
          type =  boolean
          description = ""
          optional = false
        }
        is_legendary {
          type =  boolean
          description = ""
          optional = false
        }
        is_mythical {
          type =  boolean
          description = ""
          optional = false
        }
        hatch_counter {
          type =  integer
          description = ""
          optional = false
        }
        has_gender_differences {
          type =  boolean
          description = ""
          optional = false
        }
        forms_switchable {
          type =  boolean
          description = ""
          optional = false
        }
        growth_rate {
          type = GrowthRateSummary
          description = ""
          optional = true
        }
        pokedex_numbers {
          type = array
          items.type = PokemonDexEntry
          description = ""
          optional = true
        }
