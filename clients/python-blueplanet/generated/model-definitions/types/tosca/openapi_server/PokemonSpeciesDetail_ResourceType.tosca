"$schema"   = "http://cyaninc.com/json-schemas/tosca-lite-v1/definition-module#"
title       = "openapi_server PokemonSpeciesDetail"
package     = openapi_server
version     = "1.0"
description = "Defines a PokemonSpeciesDetail"
authors     = ["F. Bar (foo@bar.baz)"]

imports {
  Root = tosca.resourceTypes.Root
  AbilityDetailPokemonInnerPokemon = openapi_server.resourceTypes.AbilityDetailPokemonInnerPokemon
  EvolutionChainSummary = openapi_server.resourceTypes.EvolutionChainSummary
  GenerationSummary = openapi_server.resourceTypes.GenerationSummary
  GrowthRateSummary = openapi_server.resourceTypes.GrowthRateSummary
  PokemonColorSummary = openapi_server.resourceTypes.PokemonColorSummary
  PokemonDexEntry = openapi_server.resourceTypes.PokemonDexEntry
  PokemonFormDetailFormNamesInner = openapi_server.resourceTypes.PokemonFormDetailFormNamesInner
  PokemonHabitatSummary = openapi_server.resourceTypes.PokemonHabitatSummary
  PokemonShapeSummary = openapi_server.resourceTypes.PokemonShapeSummary
  PokemonSpeciesDescription = openapi_server.resourceTypes.PokemonSpeciesDescription
